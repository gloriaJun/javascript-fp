{
  "name": "js-utils-fp",
  "version": "1.0.0",
  "main": "src/index.js",
  "repository": "https://github.com/gloriaJun/js-utils-fp.git",
  "license": "MIT",
  "scripts": {
    "start": "$npm_execpath run dev",
    "dev": "webpack-dev-server --open --mode development",
    "prebuild": "$npm_execpath run build:clean",
    "build": "webpack --mode production",
    "build:clean": "rimraf dist",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:watch": "$npm_execpath test --watch",
    "test:coverage": "$npm_execpath test --coverage --colors",
    "predeploy": "$npm_execpath build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint 'src/**/*.{js,jsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss}'"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --config jest.config.js --passWithNoTests"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {}
}

{
  "name": "js-utilidad",
  "version": "1.0.0",
  "author": "Gloria Jun<pureainu@gmail.com>",
  "repository": "https://github.com/gloriaJun/js-utils-fp.git",
  "license": "MIT",
  "main": "dist/js-utilidad.umd.js",
  "module": "dist/js-utilidad.es5.js",
  "typings": "dist/types/js-utilidad.d.ts",
  "files": [
    "dist"
  ],
  "keywords": [
    "javascript",
    "typescript",
    "utility"
  ],
  "scripts": {
    "start": "$npm_execpath run dev",
    "prebuild": "$npm_execpath run build:clean",
    "build": "$npm_execpath run build:umd; $npm_execpath run build:umdmin",
    "build:umd": "webpack --mode=development",
    "build:umdmin": "webpack --mode=production",
    "build:clean": "rimraf dist",
    "test": "jest --config jest.config.js --passWithNoTests",
    "test:watch": "$npm_execpath test --watch",
    "test:coverage": "$npm_execpath test --coverage --colors",
    "lint": "tsc --noEmit && eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "format": "prettier --write 'src/**/*.{js,jsx,css,scss}'"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "jest --config jest.config.js --passWithNoTests"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.1.0",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {}
}
